<p id="notice"><%= notice %></p>

<h1>Classegroups</h1>

<table>
  <thead>
    <tr>
      <th>Prenom</th>
      <th>Nom</th>
      <th>Devoirs</th>
      <th>Intra</th>
      <th>Final</th>
      <th>Total</th>
      <th>Remarques</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @classegroups.each do |classegroup| %>
      <tr>
        <td><%= classegroup.prenom %></td>
        <td><%= classegroup.nom %></td>
        <td><%= classegroup.devoirs %></td>
        <td><%= classegroup.intra %></td>
        <td><%= classegroup.final %></td>
        <td><%= classegroup.total %></td>
        <td><%= classegroup.remarques %></td>
        <td><%= link_to 'Show', classegroup %></td>
        <td><%= link_to 'Edit', edit_classegroup_path(classegroup) %></td>
        <td><%= link_to 'Destroy', classegroup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Classegroup', new_classegroup_path %>
